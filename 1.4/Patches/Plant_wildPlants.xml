<Patch>
 <!--
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BiomeDef[defName="TemperateForest" or defName="TemperateSwamp" or defName="AridShrubland" or defName="BorealForest"]/wildPlants</xpath>
		<value>
			<Plant_TreeHoney>0.015</Plant_TreeHoney>
		</value>
   </Operation>
	-->
  <Operation Class="PatchOperationSequence">
    <operations>
	<li Class="PatchOperationFindMod">
				<mods>
					<li>HSK More Content</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
					<li Class="PatchOperationAdd">
							<xpath>Defs/BiomeDef[defName="TemperateForest" or defName="TemperateSwamp" or defName="AridShrubland" or defName="BorealForest"]/wildPlants</xpath>
		                   <value>
			               <Plant_TreeHoney>0</Plant_TreeHoney>
		                  </value>
					</li>
					</operations>
				</match>
			</li>
		</operations>
	</Operation>
	
	 <Operation Class="PatchOperationSequence">
    <operations>
	<li Class="PatchOperationFindMod">
				<mods>
					<li>Alpha Bees</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
					<li Class="PatchOperationAdd">
							<xpath>Defs/BiomeDef[defName="TemperateForest" or defName="TemperateSwamp" or defName="AridShrubland" or defName="BorealForest"]/wildPlants</xpath>
		                   <value>
			               <Plant_TreeHoney>0</Plant_TreeHoney>
		                  </value>
					</li>
					</operations>
				</match>
			</li>
		</operations>
	</Operation>
	
	 <Operation Class="PatchOperationSequence">
    <operations>
	<li Class="PatchOperationFindMod">
				<mods>
					<li>Misc. Bees'n'Honey</li>
				</mods>
				<match Class="PatchOperationSequence">
					<operations>
					<li Class="PatchOperationAdd">
							<xpath>Defs/BiomeDef[defName="TemperateForest" or defName="TemperateSwamp" or defName="AridShrubland" or defName="BorealForest"]/wildPlants</xpath>
		                   <value>
			               <Plant_TreeHoney>0</Plant_TreeHoney>
		                  </value>
					</li>
					</operations>
				</match>
			</li>
		</operations>
	</Operation>

</Patch>
